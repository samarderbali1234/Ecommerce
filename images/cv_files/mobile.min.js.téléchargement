$("input[type=date]").on("change",function(b){var a=$(window).width();if(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)){if($(this).val()){$(this).prev("label.hide-date").css("display","none")}else{$("label.hide-date").css("display","inline")}}else{$("label.hide-date").style.setProperty("display","inline","important")}});$(document).on("click",".mb-menu",function(){$('nav[role="navigation"]').toggle();$(".mb-menu").not(this).removeClass("active");$(this).toggleClass("active")});function mobileSwitch(){var a=$(window).width();var d=$('nav[role="navigation"]');if($(".dob-holder select").is(":visible")){$(".datepicker").datepicker("hide")}if(a<=675){$(".shortening-txt").each(function(){$(this).html($(this).data("shortening"))})}else{$(".shortening-txt").each(function(){$(this).html($(this).data("original"))})}if(a<=670){if(!d.hasClass("mobile")){$('nav[role="navigation"]').addClass("mobile").hide();$('nav[role="navigation"]').before('<span class="mb-menu rnd-3"><i class="hamburger"></i></span>');$("li.login form").before('<i class="btn-overlay"></i>')}var c="/assets/images/avatar-upload.jpg";var b=$("#crop-img").attr("src");if(c!==b){}}else{$('nav[role="navigation"]').removeClass("mobile");$('nav[role="navigation"]').show();$(".wrapper").find("span.mb-menu").remove();$(".wrapper").find("i.btn-overlay")}if(!$('nav[role="navigation"]').find("a").length){$('nav[role="navigation"]').hide();$(".mb-menu").hide()}}$(document).ready(function(){mobileSwitch();$(window).resize(function(){mobileSwitch()})});